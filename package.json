{
    "name": "tabindentspacealign",
    "displayName": "Dynamic Tab Insertion",
    "description": "Dynamically insert spaces or tabs depending on the action (indentation or alignment)",
    "version": "1.1.1",
    "publisher": "zhiayang",
    "repository": {
        "url": "https://github.com/zhiayang/vscode-tabindentspacealign",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.dynamictabkey",
                "title": "Dynamic Tab"
            }
        ],
        "keybindings": [
            {
                "command": "extension.dynamictabkey",
                "key": "tab",
                "when": "editorTextFocus && !editorReadOnly && !inSnippetMode"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
